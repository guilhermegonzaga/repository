    "keywords": ["laravel", "eloquent", "repository", "repositories", "model", "database"],
    "license": "MIT",
    "authors": [
        {
            "name": "Guilherme de Oliveira Gonzaga",
            "email": "guilhermeoliveyra@hotmail.com"
        }
